<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="bibtex" class="org.dspace.submit.extraction.MetadataExtractor">
		<property name="dataloadersKeyMap" value="bibtex"/>	
        <property name="extensions">
            <list>
            	<value>bibtex</value>
            	<value>bib</value>
            </list>
        </property>
    </bean>

   <bean id="ris" class="org.dspace.submit.extraction.MetadataExtractor">
	<property name="dataloadersKeyMap" value="ris"/>
        <property name="extensions">
            <list>
            	<value>RIS</value>
            	<value>ris</value>
            </list>
        </property>
    </bean>

    <bean id="wosris" class="org.dspace.submit.extraction.MetadataExtractor">
	<property name="dataloadersKeyMap" value="wosris"/>
        <property name="extensions">
            <list>
                <value>CIW</value>
                <value>ciw</value>
            </list>
        </property>
    </bean>

    <bean id="endnote" class="org.dspace.submit.extraction.MetadataExtractor">
			<property name="dataloadersKeyMap" value="endnote"/>
        <property name="extensions">
            <list>
            	<value>enw</value>
            </list>
        </property>
    </bean>
    
    <bean id="grobid" class="org.dspace.submit.extraction.MetadataExtractor">
					<property name="dataloadersKeyMap" value="grobid"/>
        <property name="extensions">
            <list>
            	<value>pdf</value>
            </list>
        </property>
    </bean>

 
 </beans>